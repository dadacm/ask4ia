{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@base/*": [
        "./base/*"
      ],
      "@env": [
        "node_modules/react-native-dotenv"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "nativewind-env.d.ts",
    "base/types/reactotron.d.ts",
    "base/types/env.d.ts"
  ]
}
